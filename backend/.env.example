# Application
APP_ENV=development
HTTP_PORT=8888

# Database (postgres | mysql)
DB_DRIVER=postgres
DB_HOST=127.0.0.1
DB_PORT=5432
DB_USER=finance
DB_PASSWORD=finance
DB_NAME=finance
DB_SSLMODE=disable
DB_TIMEZONE=Asia/Shanghai

# Auth (single admin user)
AUTH_USERNAME=admin
# Use bcrypt hash, e.g. from `openssl passwd -6` won't work; use `htpasswd -nbBC 10 "" "yourpass" | tr -d ':\n'`
AUTH_PASSWORD_HASH=
AUTH_JWT_SECRET=please-change
